# Snapshot UI Configuration for Custom Backend Integration
# Point to our adapter service instead of official Snapshot API

# Main API endpoint - points to our adapter GraphQL endpoint
VITE_UNIFIED_API_URL=http://localhost:4001/graphql
VITE_UNIFIED_API_TESTNET_URL=http://localhost:4001/graphql

# Network Configuration - Enable Ethereum networks that use our adapter
VITE_DEFAULT_NETWORK=eth
VITE_ENABLED_NETWORKS=eth,sep,matic,arb1,base

# Metadata network for user profiles, etc. - Use eth instead of 's'
VITE_METADATA_NETWORK=eth

# Optional: Disable features we don't need
VITE_ENABLE_GOVERNOR_BRAVO=false
